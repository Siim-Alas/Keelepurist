@page
@model KeelepuristMain.AdminTestGlossaryModel
@{
    ViewData["Title"] = "TestExerciseList";
}

<h1>Kontrolltööde haldus</h1>

<a asp-page="./AdminTestEditor">Loo uus kontrolltest</a>

<ul>
    @foreach (var path in Model.TestExercises)
    {
        <li>
            <a asp-page="./AdminTestEditor" asp-route-blobName="@path">@path</a>
        </li>
    }
</ul>

<a class="btn btn-light" asp-page="./AdminIndex">Tagasi haldusesilehele</a>